<idea-plugin>
    <id>net.fenstonsingel.fcbp</id>
    <name>fcbp</name>
    <vendor>fenstonsingel</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="net.fenstonsingel.fcbp.services.MyApplicationService"/>
        <projectService serviceImplementation="net.fenstonsingel.fcbp.services.MyProjectService"/>
    </extensions>

    <applicationListeners>
        <listener class="net.fenstonsingel.fcbp.listeners.MyProjectManagerListener"
                  topic="com.intellij.openapi.project.ProjectManagerListener"/>
    </applicationListeners>
</idea-plugin>
